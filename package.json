{
  "license": "MIT",
  "devDependencies": {
    "@sitegeist/es-neos-stylelint": "^2.0.2",
    "@sitegeist/eslint-config-es-neos-eslint": "^2.0.2",
    "@types/css-minimizer-webpack-plugin": "^1.1.1",
    "@types/jest": "^26.0.20",
    "@types/mini-css-extract-plugin": "^1.2.2",
    "@types/node": "^14.14.31",
    "@types/terser-webpack-plugin": "^5.0.2",
    "autoprefixer": "^10.2.4",
    "browserslist": "^4.16.3",
    "css-loader": "^5.1.1",
    "css-minimizer-webpack-plugin": "^1.2.0",
    "eslint": "^7.21.0",
    "glob": "^7.1.6",
    "jest": "^26.6.3",
    "mini-css-extract-plugin": "^1.3.9",
    "node": "^14",
    "postcss": "^8.2.6",
    "postcss-custom-media": "^8.0.0",
    "postcss-discard-duplicates": "^4.0.2",
    "postcss-loader": "^5.0.0",
    "postcss-media-minmax": "^5.0.0",
    "postcss-mixins": "^7.0.3",
    "postcss-modules": "^4.0.0",
    "precss": "^4.0.0",
    "stylelint": "^13.11.0",
    "ts-jest": "^26.5.2",
    "ts-loader": "^8.0.17",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.2",
    "webpack": "^5.24.2",
    "webpack-cli": "^4.5.0"
  },
  "scripts": {
    "cleanup:node": "find . -name 'node_modules' -type d -prune -print -exec rm -rf '{}' \\;",
    "build": "webpack --mode production",
    "watch": "webpack -w --mode development",
    "lint:css": "stylelint 'DistributionPackages/*/Resources/Private/**/*.css'",
    "lint:js": "eslint 'DistributionPackages/*/Resources/Private/**/*.ts'"
  },
  "dependencies": {},
  "browserslist": [
    ">1%",
    "not dead",
    "not IE 11",
    "not IE_Mob 11"
  ],
  "eslintConfig": {
    "extends": [
      "@sitegeist/es-neos-eslint"
    ],
    "parser": "@typescript-eslint/parser"
  },
  "stylelint": {
    "extends": [
      "@sitegeist/es-neos-stylelint"
    ],
    "rules": {
      "property-no-unknown": [
        true,
        {
          "ignoreProperties": [
            "composes"
          ]
        }
      ]
    }
  },
  "jest": {
    "preset": "ts-jest",
    "testMatch": [
      "<rootDir>/DistributionPackages/*/Resources/Private/Fusion/**/*.spec.ts"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "modulePathIgnorePatterns": [
      "<rootDir>/Packages/"
    ]
  }
}
